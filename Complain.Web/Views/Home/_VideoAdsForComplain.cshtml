@model IEnumerable<Complain.Entities.Entities.VideoAds>
@{
    Layout = null;
}

<div class="row align-items-center">
    @foreach (var item in Model)
    {
        <div class="col-md-6 mb-4 mb-md-0 pr-md-5">
            <h2 class="heading"><a href="@item.WebSite">@item.CompanyName</a></h2>
            <p>@item.Title</p>
        </div>
        <div class="col-md-6">
            <iframe src="@item.VideoLink" style="max-height:170px;max-width:250px;min-height:170px;min-width:250px" frameborder="0" allowtransparency="false"></iframe>
        </div>

    }
</div>

